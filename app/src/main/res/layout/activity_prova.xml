<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="online.padev.kariti.ProvaActivity">

    <include
        android:id="@+id/include"
        layout="@layout/appbarquestion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/buttonCadProva"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:background="@drawable/button_selector"
        android:padding="5dp"
        android:text="@string/buttonCadProva"
        android:textColor="@color/cinzaClaro"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include" />

    <Button
        android:id="@+id/buttonGerarCatao"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:background="@drawable/button_selector"
        android:padding="5dp"
        android:text="@string/buttonGerarCartao"
        android:textColor="@color/cinzaClaro"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCadProva" />

    <Button
        android:id="@+id/buttonCorrigirProva"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/button_selector"
        android:padding="5dp"
        android:text="@string/buttonCorrigirProva"
        android:textColor="@color/cinzaClaro"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonGerarCatao" />

    <Button
        android:id="@+id/buttonVisuProva"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/button_selector"
        android:text="@string/buttonVisuProva"
        android:textColor="@color/cinzaClaro"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCorrigirProva"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonEdicaoProva"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/button_selector"
        android:text="@string/textViewEditProva"
        android:textColor="@color/cinzaClaro"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonVisuProva"
        app:layout_constraintVertical_bias="0.015" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:contentDescription="@string/imgBtnSeta"
        app:layout_constraintBottom_toBottomOf="@+id/buttonCadProva"
        app:layout_constraintEnd_toEndOf="@+id/buttonCadProva"
        app:layout_constraintTop_toTopOf="@+id/buttonCadProva"
        app:srcCompat="@mipmap/ic_arrownext" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:contentDescription="@string/imgBtnSeta"
        app:layout_constraintBottom_toBottomOf="@+id/buttonGerarCatao"
        app:layout_constraintEnd_toEndOf="@+id/buttonGerarCatao"
        app:layout_constraintTop_toTopOf="@+id/buttonGerarCatao"
        app:srcCompat="@mipmap/ic_arrownext" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:contentDescription="@string/imgBtnSeta"
        app:layout_constraintBottom_toBottomOf="@+id/buttonCorrigirProva"
        app:layout_constraintEnd_toEndOf="@+id/buttonCorrigirProva"
        app:layout_constraintTop_toTopOf="@+id/buttonCorrigirProva"
        app:srcCompat="@mipmap/ic_arrownext" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:contentDescription="@string/imgBtnSeta"
        app:layout_constraintBottom_toBottomOf="@+id/buttonVisuProva"
        app:layout_constraintEnd_toEndOf="@+id/buttonVisuProva"
        app:layout_constraintTop_toTopOf="@+id/buttonVisuProva"
        app:layout_constraintVertical_bias="0.481"
        app:srcCompat="@mipmap/ic_arrownext" />

    <ImageView
        android:contentDescription="@string/iconeProva"
        android:id="@+id/imageView12"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonCadProva"
        app:layout_constraintStart_toStartOf="@+id/buttonCadProva"
        app:layout_constraintTop_toTopOf="@+id/buttonCadProva"
        app:srcCompat="@mipmap/prova" />

    <ImageView
        android:contentDescription="@string/iconeProva"
        android:id="@+id/imageView14"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonGerarCatao"
        app:layout_constraintStart_toStartOf="@+id/buttonGerarCatao"
        app:layout_constraintTop_toTopOf="@+id/buttonGerarCatao"
        app:srcCompat="@mipmap/prova" />

    <ImageView
        android:contentDescription="@string/iconeCorrigir"
        android:id="@+id/imageView15"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonCorrigirProva"
        app:layout_constraintStart_toStartOf="@+id/buttonCorrigirProva"
        app:layout_constraintTop_toTopOf="@+id/buttonCorrigirProva"
        app:srcCompat="@mipmap/edit" />

    <ImageView
        android:contentDescription="@string/iconeSearch"
        android:id="@+id/imageView13"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonVisuProva"
        app:layout_constraintStart_toStartOf="@+id/buttonVisuProva"
        app:layout_constraintTop_toTopOf="@+id/buttonVisuProva"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@mipmap/search" />

    <View
        android:id="@+id/divider4"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toBottomOf="@+id/include"
        tools:layout_editor_absoluteX="2dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>